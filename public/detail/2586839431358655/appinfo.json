a_disclosure_translated": null, "__isItemWithComfortRating": "Application", "comfort_rating": "COMFORTABLE_FOR_MOST", "is_viewer_subscribed_coming_soon": false, "internet_connection": "REQUIRED_FOR_DOWNLOAD", "internet_connection_name": "下载内容需要连接网络", "__isItemWithHmdPlatformInformaton": "Application", "supported_input_devices_list": [{"name": "Oculus Touch", "tag": "OCULUS_TOUCH"}], "canonicalName": "whalerock-industries-testedvr-android6d0f", "viewer_has_preorder": false, "purchase_alerts": [], "is_granted_on_hardware_registration": false, "current_trial_offer": null, "extra_buy_box_section": null, "is_viewer_entitled": false, "baseline_offer": {"price": {"offset_amount": "0"}, "id": "376938749665167"}, "__isWishlistableItem": "Application", "entry": null, "is_demo_of": null, "appName": "Adam Savage's Tested VR", "platformOffer": null, "is_giftable": false, "current_gift_offer": null, "externalReviews": {"count": 0}, "quality_rating_aggregate": 3.8214285714286, "support_website_url": "https://www.tested.com/", "quality_ratings_by_viewer": [], "reviewInfo": {"count": 72, "orderings": [{"key": "top", "label": "最相关"}, {"key": "most_recent", "label": "从新到旧"}]}, "firstQualityRatings": {"count": 72, "page_info": {"end_cursor": "AQHRiMz9DrVn3pfEAJ_T8iQCXRtb0rodNJrIrI7PEKMhpoQth9LmOIabdz_Rrinl7AMYHKksXKsCj6QubXGhJUv00g", "start_cursor": "AQHRiMz9DrVn3pfEAJ_T8iQCXRtb0rodNJrIrI7PEKMhpoQth9LmOIabdz_Rrinl7AMYHKksXKsCj6QubXGhJUv00g"}, "edges": [{"node": {"id": "3434132393302068", "author": {"id": "832650133450320", "display_name": "DK_VR", "profile_photo": {"uri": "https://scontent.oculuscdn.com/v/t64.5771-25/q92/c0.0.602.602a/s160x160/38974879_3383776061671035_8191995135400623891_n.jpg?_nc_cat=104&ccb=2&_nc_sid=79b88e&_nc_ohc=SCdNkMJG3AoAX_K-VR3&_nc_ht=scontent.oculuscdn.com&tp=28&oh=e96111c783ae8557df29404b8820dd9c&oe=600DA798"}}, "score": 5, "reviewDescription": "UPDATE: there are some really sad and just plain stupid people giving a poor rating becuase they can't read or do even the slightest research. This isn't a game. It's clearly outlined throughout nthr store page. It's also free so please tuck tail and just uninstall it. We don't need a public display of your stupidity while also baslessly lowering the review score on such a unique and polished app. \n\nI love Tested and I've loved Adam Savage since the Mythbuster days. This app is fantastic. The presentation is excellent and the video quality is some of the best 3d Stereo out right now. Period. I've always wanted to see Adam's shop in VR. I was just talking to someone about watching an Untitled podcast episode or a one day buil in VR and this comes out of nowhere. It's above and beyond what I wanted. There's so much to see. His shop is the perfect place for 3D video. There's so much to see and so many layers to his stuff that it gives off some of the best depth and I swear my eyes were playing tricks. I could sense the parallax in some shots. \n\nThe video files are really large, however, they download in a few minutes. It's all very impressive. \n\nThis should be in everyone's library even if you don't know what tested is. This is a glimpse into the future of how we can get involved in our favorite creator's lives. We can be there with them, work along side them. It's an intimacy like never before. It's exciting stuff.", "reviewTitle": "A fantastic experience for everyone", "review_helpful_count": 55, "review_rank_by_viewer": null, "date": 1607064630, "developerResponse": null}}]}, "can_viewer_review": false, "is_offer_region_restricted": false, "__isWithWebsitePageMeta": "Application", "website_page_meta": {"description": "Adam Savage's Tested takes you inside the workshops and maker spaces of incredible creators to learn their processes and watch their awesome builds.", "image_height": 405, "image_type": "image/jpeg", "image_url": "https://scontent.oculuscdn.com/v/t64.5771-25/q92/s720x720/38974464_387332601889342_6041149162869751808_n.jpg?_nc_cat=1&ccb=2&_nc_sid=79b88e&_nc_o