上探寻 Activitude 的进化史，从虚拟实境初创公司到今天的“人类意义提供商”。提供超过 50 种可探索的虚拟实境 长达三小时的独特游戏体验 取下和戴上不同的头戴设备可进行深度探索", "developer_privacy_policy_url": "http://tenderclaws.com/privacy/PrivacyPolicy.html", "developer_terms_of_service_url": "http://tenderclaws.com/privacy/EULA.html", "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "publisher_name": "Tender Claws", "supported_in_app_languages": [{"name": "中文（香港）"}, {"name": "中文（台湾）"}, {"name": "德语"}, {"name": "意大利语"}, {"name": "日语"}, {"name": "法语（法国）"}, {"name": "英语"}, {"name": "西班牙语（墨西哥）"}, {"name": "韩语"}], "supported_hmd_platforms": ["MONTEREY"], "supported_input_device_names": ["Oculus Touch"], "user_interaction_mode_names": ["单人"], "age_rating": null, "category_name": "游戏", "developer_name": "Tender Claws", "genre_names": ["休闲", "探索", "故事"], "is_360_sensor_setup_required": false, "is_concept": false, "latest_supported_binary": {"__typename": "AndroidBinary", "version": "1.8.13", "change_log": "buttery vacuuming goodness", "supported_hmd_types": ["MONTEREY"], "total_installed_space": "539080490", "id": "2449931405070912", "required_space_adjusted": "729541668", "storeAddOns": null, "firstIapItems": null}, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "release_date": 1558458000, "supported_tracking_modes": [], "website_url": "https://tenderclaws.com/vvr", "current_offer": {"__typename": "AppStoreOffer", "offer_type": "APPSTORE_OFFER", "id": "837799547022133", "__isPurchasableOfferWithApp": "AppStoreOffer", "price": {"currency": "USD", "offset_amount": "999", "formatted": "$9.99"}, "strikethrough_price": {"currency": "USD", "offset_amount": "1499", "formatted": "$14.99", "offset": 100}, "promo_benefit": "-33%", "end_time": 1609142340, "show_timer": false, "buy_button_captions": []}, "viewer_release_channel": {"id": "609893526111037"}, "live_release_channel": {"id": "609893526111037", "channel_name": "LIVE", "latest_supported_binary": {"__typename": "AndroidBinary", "id": "2449931