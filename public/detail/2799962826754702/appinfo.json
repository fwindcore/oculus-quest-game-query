山森林体验到前所未有的恐怖气氛。\n \n这片受到诅咒的丛林将会唤醒你内心最深层的恐惧。", "developer_privacy_policy_url": "https://blairwitchgame.com/docs/privacy-policy", "developer_terms_of_service_url": "https://blairwitchgame.com/upload/terms_and_conditions.pdf", "supported_player_modes": ["SITTING", "STANDING", "ROOM_SCALE"], "publisher_name": "Bloober Team NA", "supported_in_app_languages": [{"name": "中文（简体）"}, {"name": "俄语"}, {"name": "德语"}, {"name": "意大利语"}, {"name": "日语"}, {"name": "法语（法国）"}, {"name": "波兰语"}, {"name": "英语"}, {"name": "葡萄牙语（巴西）"}, {"name": "西班牙语（西班牙）"}], "supported_hmd_platforms": ["MONTEREY"], "supported_input_device_names": ["Oculus Touch"], "user_interaction_mode_names": ["单人"], "age_rating": null, "category_name": "游戏", "developer_name": "Bloober Team", "genre_names": ["冒险", "探索", "恐怖"], "is_360_sensor_setup_required": false, "is_concept": false, "latest_supported_binary": {"__typename": "AndroidBinary", "version": "1.46", "change_log": "", "supported_hmd_types": ["MONTEREY"], "total_installed_space": "3976647272", "id": "3535979193153058", "required_space_adjusted": "7687807522", "storeAddOns": null, "firstIapItems": null}, "recommended_graphics": null, "recommended_memory_gb": null, "recommended_processor": null, "release_date": 1603990800, "supported_tracking_modes": [], "website_url": "https://blairwitchgame.com/", "current_offer": {"__typename": "AppStoreOffer", "offer_type": "APPSTORE_OFFER", "id": "2739439339603150", "__isPurchasableOfferWithApp": "AppStoreOffer", "price": {"currency": "USD", "offset_amount": "2999", "formatted": "$29.99"}, "strikethrough_price": null, "promo_benefit": null, "end_time": 0, "show_timer": false, "buy_button_captions": []}, "viewer_release_channel": {"id": "217622916266509"}, "live_release_channel": {"id": "217622916266509", "channel_name": "LIVE", "latest_supported_binary": {"__typename": "AndroidBinary", "id": "3535979193153058"}}, "release_channels": {"count": 1, "nodes": [{"channel_name": "LIVE", "id": "217622916266509", "latest_supported